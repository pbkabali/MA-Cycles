<table>
  <thead>
    <tr>
      <th colspan="1"></th>
      <th colspan="1"></th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= image_tag(avatar_url(user.Photo), :class =>'avatar')%></td>
        <td class='author color-auto'><%= user.FullName %></td>
        <td><%= link_to 'See Profile', user %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>